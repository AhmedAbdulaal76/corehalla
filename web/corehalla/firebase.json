{
    "hosting": {
        "target": "corehalla",
        "public": "dist",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**", "dist/index.html"],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "cleanUrls": true,
        "trailingSlash": false
    },
    "functions": {
        "runtime": "nodejs10"
    }
}
