{
    "name": "db",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "db:migrate": "yarn db:supabase:clean && prisma migrate dev && yarn db:supabase:setup",
        "db:migrate:prod": "prisma migrate deploy && yarn db:supabase:setup",
        "db:migrate:create": "yarn db:supabase:clean && prisma migrate dev --create-only && yarn db:supabase:setup",
        "db:supabase:setup": "yarn db:rls:setup && yarn db:realtime:setup yarn db:auth-user-trigger:add",
        "db:supabase:clean": "yarn db:auth-user-trigger:rm",
        "db:auth-user-trigger:add": "prisma db execute --preview-feature --file ./prisma/auth_user_trigger_add.sql --schema ./prisma/schema.prisma",
        "db:auth-user-trigger:rm": "prisma db execute --preview-feature --file ./prisma/auth_user_trigger_rm.sql --schema ./prisma/schema.prisma",
        "db:rls:setup": "prisma db execute --preview-feature --file ./prisma/rls_setup.sql --schema ./prisma/schema.prisma",
        "db:realtime:setup": "prisma db execute --preview-feature --file ./prisma/realtime_setup.sql --schema ./prisma/schema.prisma",
        "format": "prisma format",
        "db:studio": "prisma studio",
        "ts:check": "tsc --noEmit"
    },
    "devDependencies": {
        "@types/react": "^18.0.5",
        "discord-api-types": "^0.31.2",
        "prisma": "^3.7.0",
        "tsconfig": "*",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "@prisma/client": "^3.7.0",
        "@supabase/supabase-js": "^1.34.1",
        "axios": "^0.26.1",
        "react": "^18.0.0",
        "react-query": "^3.35.0",
        "logger": "*"
    }
}
