{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "plugins": ["@babel"]
}
